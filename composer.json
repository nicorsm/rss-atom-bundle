{
    "name": "debril/rss-atom-bundle",
    "type": "symfony-bundle",
    "description": "RSS and Atom support for Symfony 2",
    "keywords": ["rss", "atom"],
    "license": "LGPL",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:samuele-mrapps/rss-atom-bundle.git"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/config": "~2.2",
        "symfony/options-resolver": "~2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "doctrine/common": "~2.3",
	"symfony/framework-bundle": "~2.3",
        "symfony/finder": "~2.3",
        "symfony/form": "~2.3",
        "symfony/browser-kit": "~2.3"
    },
    "autoload": {
        "psr-0": {
            "Debril\\RssAtomBundle": ""
        }
    },
    "target-dir": "Debril/RssAtomBundle",
    "config": {
        "bin-dir": "bin"
    }
}
